  "homepage": "https://github.com/eggjs/egg-zookeeper#readme",
  "engines": {
    "node": ">= 6.0.0"
  },
  "ci": {
    "version": "6, 8"
  },
  "dependencies": {
    "zookeeper-cluster-client": "^1.0.2"
  },
  "devDependencies": {
    "autod": "^2.9.0",
    "autod-egg": "^1.0.0",
    "egg": "^1.7.0",
    "egg-bin": "^4.1.0",
    "egg-mock": "^3.9.1",
    "eslint": "^4.4.1",
    "eslint-config-egg": "^5.0.0",
    "mm": "^2.1.0"
