{
  "name": "egg-zookeeper",
  "version": "1.0.0",
  "description": "egg plugin for zookeeper",
  "main": "app.js",
  "files": [
    "app",
    "lib",
    "app.js",
    "agent.js"
  ],
  "eggPlugin": {
    "name": "zookeeper"
  },
  "scripts": {
    "autod": "autod",
    "lint": "eslint . --ext .js",
